[user]
	name = jonafll
	email = contact@jonafellisch.com
	signingKey = E1F2ED52206682E0

[init]
	defaultBranch = master

[commit]
	gpgsign = true

[tag]
	gpgSign = true

[alias]
	st = status
	cm = commit -m
	cma = commit -am
	co = checkout
	cob = checkout -b
	sw = switch
	swc = switch -c
	del = branch -D
	rv = remote -v
	res = !git reset --hard
	done = !git push origin HEAD
	br = branch -vv
	unstage = reset HEAD --
	last = log -1 HEAD --stat
	undo = reset HEAD~1 --mixed
	ls = log --oneline
	lal = log --oneline --graph --branches
	conf = config --global -l
	wip = commit -am WIP --no-verify
	
[pull]
	ff = only
